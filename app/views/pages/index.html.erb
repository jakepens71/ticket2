<%= javascript_include_tag '//code.highcharts.com/highcharts.js' %>
<%= javascript_include_tag '//code.highcharts.com/highcharts-more.js' %>
<%= javascript_include_tag '//code.highcharts.com/modules/exporting.js' %>
<%= javascript_include_tag 'pages.js' %>

<% if !user_signed_in? %>
	<% content_for :top_content do %>
	  <div class="jumbotron" class="center">
	    <div class="container">
	      <h1>Welcome to Penn United Technolgies Ticket System!</h1>
	      <p>Register to get started.</p>
	      <p>
		<%= link_to 'Register', new_user_registration_path, class: 'btn btn-primary btn-lg' %>
		<%= link_to "Sign In", new_user_session_path, class: 'btn btn-primary btn-lg' %>
	      </p>
	    </div>
	  </div>
	<% end %>
<% else %>
	<div id="container" style="width:100%; height:400px;"></div>
<% end %>